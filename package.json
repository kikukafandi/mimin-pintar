{
  "name": "mimin-pintar",
  "version": "1.0.0",
  "description": "WhatsApp Bot AI",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "build": "tsc src/main.ts --outDir dist --module commonjs --target es2020 --esModuleInterop --skipLibCheck --strict false",
    "package": "pkg dist/main.js --targets node18-win-x64 --output MiminPintar.exe"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "body-parser": "^2.2.2",
    "express": "^5.2.1",
    "open": "^11.0.0",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@swc/core": "^1.15.10",
    "@types/express": "^5.0.6",
    "@types/qrcode": "^1.5.6",
    "@vercel/ncc": "^0.38.4",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "data/**/*",
      "public/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "./"
  }
